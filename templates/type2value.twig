{% spaceless %}
    {% if type.type == 'int' %}
        0
    {% elseif type.type == 'float' %}
        0.0
    {% elseif type.type == 'string' %}
        ''
    {% elseif type.type == 'array' %}
        array()
    {% elseif type.type == 'object' and type.className is defined %}
        $this->getMockBuilder('{{type.className}}')->getMock()
    {% else %}
        null
    {% endif %}
{% endspaceless %}